{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "esModuleInterop": true
  },
  "include": ["vite.config.ts", "src", "node_modules/**/*.ts"]
}